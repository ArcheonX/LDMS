@{
    ViewData["Title"] = "User Management";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section NavHeader{
    <h3 class="text-themecolor m-b-0 m-t-0">I-Manage</h3>
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="javascript:void(0)">I-Manage LDMS</a></li>
    </ol>
}
<script src="~/assets/app/ImanageLDMS.js"></script>
<div class="panel panel-primary" style="width:100%">
    <div class="panel-body panel-body-begin">
        <div class="form-horizontal">
            <div class="row form-group">
                <div class="col-sm-2 col-md-2">
                    <button id="btnUser" class="btn btn-info waves-effect waves-light" style="width:100%"> User </button>
                </div>
                <div class="col-sm-2 col-md-2" style="margin-left:-25px">
                    <button id="btnMasterReport" class="btn btn-info btn-inactive waves-effect waves-light" style="width:100%">Master Report </button>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="panel panel-primary" style="margin-top:-15px;width:100%;">
    <div id="viewAllUser" style="display:block;width:100%">
        @Html.Partial("_SearchEmployeeCritiria")
        <div id="UserListcontent">
            @Html.Partial("_ViewAllUser")
        </div>
    </div>
    <div id="UserEditor" style="display:none;width:100%;">
        @Html.Partial("_UserEditor")
    </div>
    <div id="MasterReport" style="display:none;width:100%;">
        @Html.Partial("_MasterReport")
    </div>
</div>
@*border-width:thin;border-style:solid;*@