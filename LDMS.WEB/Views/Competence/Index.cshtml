@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/assets/app/CompetenceAnalytic/Analytic.js"></script>
<div class="panel panel-primary" style="margin-top:-15px;width:100%">
    <div class="panel-body panel-body-begin">
        <div id="pnCommandAnalytic" style="width: 100%;display:block">
            <div class="form-horizontal">
                <div class="row form-group" style="padding-top:5px;">
                    <div class="col-sm-2 col-md-2 offset-10">
                        <button id="btnAddCompetence" class="btn btn-outline-success waves-effect waves-light" style="width:90%">
                            <img style="width:25px;height:20px" src="~/assets/images/svg/add-button-circle.svg" /> Add Competence
                        </button>
                    </div>
                </div>
                <div class="row form-group" style="padding-top:15px;margin-left:15px;margin-right:15px;">
                    <table class="table table-striped table-hover table-fixed" id="dtCompetenceList" style="width:100%;border:thin;">
                        <thead>
                            <tr>
                                <th style="text-align:center;width:100px">#</th>
                                <th style="text-align:center">Competence Platform</th>  
                                <th style="text-align:center;width:100px">Edit</th>
                                <th style="text-align:center;width:100px">Analytic</th>
                            </tr>
                        </thead>
                        <tbody> </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

